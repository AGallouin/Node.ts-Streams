{
  "name": "node.ts-streams",
  "version": "1.0.2",
  "description": "Node Streams with typescript",
  "main": "dist/streams.js",
  "types" : "dist/streams.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "watch-test": "jest --watch",
    "prepublish": "npm run build"
  },
  "author": "AGallouin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AGallouin/Node.ts-Streams/issues"
  },
  "files": ["dist/**/*"],
  "homepage": "https://github.com/AGallouin/Node.ts-Streams#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.20",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  }
